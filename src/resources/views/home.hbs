<div class="mt-4">
    <div class="row">
        {{#each courses}}
        <div class="col-sm-6 col-lg-4">
            <div class="card">
            <img src="{{this.img}}" class="card-img-top" alt="IMG">
                <div class="card-body">
                    <h5 class="card-title">{{this.name}}</h5>
                    <p class="card-text">{{this.des}}</p>
                    <a href="#" class="btn btn-primary">Tìm hiểu</a>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>